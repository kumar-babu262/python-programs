n=int(input())
l=list(map(int,input().split()))
l1=[]
l2=[]
if len(l)%2==0:
 for i in range(n-1,(len(l)//2)-1,-1):
    l1.append(l[i])
 j=0
 for i in range((len(l)//2),n):
   l[i]=l1[j]
   j+=1
if len(l)%2==1:
 for i in range(n-1,len(l)//2,-1):
    l1.append(l[i])
 j=0
 for i in range((len(l)//2)+1,n):
   l[i]=l1[j]
   j+=1

print(l)

